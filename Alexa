#include <stdio.h>

int main() {

    float num;

    scanf("%f", &num);

    // Output with 6 decimal digits (default for %f)

    printf("%f\n", num);

    // Output with 4 decimal digits

    printf("%.4f\n", num);

    // Output with 2 decimal digits

    printf("%.2f\n", num);

    // Output with 0 decimal digits (rounded)

    printf("%.0f\n", num);

    return 0;

}
