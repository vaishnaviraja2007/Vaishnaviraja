#include <stdio.h>

int calculateSquare(int n) {

    return n * n;

}

int main() {

    int n;

    scanf("%d", &n);

    if(n >= 1 && n <= 9) {

        int sq = calculateSquare(n);

        printf("Square of %d is %d\n", n, sq);

    } else {

        printf("Invalid Input\n");

    }

    return 0;

}
