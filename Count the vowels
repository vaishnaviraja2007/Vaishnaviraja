#include <stdio.h>
#include <ctype.h>
int countVowels(char word[]) {
    int count = 0;
    for (int i = 0; word[i] != '\0'; i++) {
        char ch = tolower(word[i]);  
        if (ch == 'a' || ch == 'e' || ch == 'i' ||
            ch == 'o' || ch == 'u') {
            count++;
        }
    }
    return count;
}
int main() {
char input[100];
scanf("%s", input);  
    int vowelCount = countVowels(input);
 printf("count: %d\n", vowelCount);
 return 0;
}
