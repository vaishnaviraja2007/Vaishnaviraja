#include <stdio.h>

int main() {

    double x1, y1, x2, y2;

    // Read the input values

    scanf("%lf %lf %lf %lf", &x1, &y1, &x2, &y2);

    // Calculate the midpoint coordinates

    double midX = (x1 + x2) / 2.0;

    double midY = (y1 + y2) / 2.0;

    // Display the result

    printf("Binoy's house is located at (%.1lf,%.1lf)\n", midX, midY);

    return 0;

}
