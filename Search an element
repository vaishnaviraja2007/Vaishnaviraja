#include <stdio.h>

int main() {
    int n, i, search_element;
    int found = 0;

    scanf("%d", &n);

    int arr[n];

    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    scanf("%d", &search_element);

    for (i = 0; i < n; i++) {
        if (arr[i] == search_element) {
            found = 1;
            break;
        }
    }

    if (found) {
        printf("%d is presented in an array.\n", search_element);
    } else {
        printf("%d is not presented in an array.\n", search_element);
    }

    return 0;
}
