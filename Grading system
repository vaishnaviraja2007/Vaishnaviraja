#include<stdio.h>

int main()

    {

    char name[20];

    int a,b,c,d,e,mark;

    float avg;

    scanf(" %s", name);

    printf("Name of the Student:%s\n",name);

    scanf(" %d", &a);

    scanf(" %d", &b);

    scanf(" %d", &c);

    scanf(" %d", &d);

    scanf(" %d", &e);

    mark = a+b+c+d+e;

    printf("Total Mark:%d\n", mark);

    avg=mark/5.0;

    printf("Average Mark:%.1f\n",avg);

    

    if(avg >= 90)

        {

        printf("Grade Mark:A");

        }

    else if (avg >= 80 )

        {

        printf("Grade Mark:B");

        }

    else if (avg >= 70)

        {

        printf("Grade Mark:C");

        }

    else if (avg >= 60)

        {

        printf ("Grade Mark:D");

        }

    else if (avg >= 50)

        {

        printf("Grade Mark:E");

        }

    else

        {

        printf("Grade Mark:Fail");

        }

               return 0;

               }
